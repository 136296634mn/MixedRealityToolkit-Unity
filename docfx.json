{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.sln", "**/*.cs" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "Assets/MixedRealityToolkit"
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml"],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": ["api_doc/**", "Documentation/**/*.md", "*.md", "*.yml", "toc.yml", "Documentation/**/*.yml", "Documentation/Architecture/**", "Assets/MixedRealityToolkit/**/*.md"]
      }
    ],
    "resource": [
      {
        "files": [ "External/**", "index.html", "Documentation/Architecture/SpatialAwareness/Images/**"]
      }
    ],
    "overwrite": "specs/*.md",
    "globalMetadata": {
      "_appTitle": "Mixed Reality Toolkit Documentation",
      "_enableSearch": true,
	  "_appLogoPath": "External/ReadMeImages/mrt_logo_icon.png",
	  "_appFaviconPath": "External/ReadMeImages/favicon.ico"
    },
    "markdownEngineName": "markdig",
    "dest": "doc",
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}
